                                            <h6 class="plan-name">Freelancer</h6>
                                            <div class="plan-price">
                                                <small class="currency">$</small>
                                                <div class="month-price animated preFadeInUp fadeInUp">
                                                    <span>15</span>
                                                    <small>/ month</small>
                                                </div>
                                                <div class="year-price animated preFadeInUp fadeInUp is-hidden">
                                                    <span>180</span>
                                                    <small>/ year</small>
                                                </div>
                                            </div>
                                            <ul>
                                                <li><b>60</b> Invoices per month</li>
                                                <li><b>15 Gb</b> Storage</li>
                                                <li><b>1</b> User account</li>
                                                <li><b>∞</b> Clients</li>
                                            </ul>
                                            <a href="#" class="button button-cta primary-btn btn-outlined is-bold">Sign up</a>
                                        </div>
                                    </div>
                                    <!-- /Pricing table transparent -->
                                </div>
                            </div>
                        </div>
                        <!-- /Freelance pricing -->
        
                        <!-- Business pricing -->
                        <div id="business-pricing" class="side-pricing is-hidden">
                            <div class="columns is-vcentered">
                                <div class="column">
                                    <!-- Pricing table featured -->
                                    <div class="pricing-card raised animated preFadeInLeft fadeInLeft">
                                        <div class="popular"><i class="im im-icon-Crown-2"></i></div>
                                        <div class="inner">
                                            <h6 class="plan-name">Pro</h6>
                                            <div class="plan-price">
                                                <small class="currency">$</small>
                                                <div class="month-price animated preFadeInUp fadeInUp">
                                                    <span>49</span>
                                                    <small>/ month</small>
                                                </div>
                                                <div class="year-price animated preFadeInUp fadeInUp is-hidden">
                                                    <span>588</span>
                                                    <small>/ year</small>
                                                </div>
                                            </div>
                                            <ul>
                                                <li><b>500 +</b> Invoices per month</li>
                                                <li><b>50 Gb</b> Storage</li>
                                                <li><b>10</b> User accounts</li>
                                                <li><b>∞</b> Clients</li>
                                            </ul>
                                            <a href="#" class="button button-cta btn-align primary-btn raised">Sign up</a>
                                        </div>
                                    </div>
                                    <!-- /Pricing table featured -->
                                </div>
                                <div class="column">
                                    <!-- Pricing table transparent -->
                                    <div class="pricing-card transparent animated preFadeInLeft fadeInLeft delay-1">
                                        <div class="inner">
                                            <h6 class="plan-name">Business</h6>
                                            <div class="plan-price">
                                                <small class="currency">$</small>
                                                <div class="month-price animated preFadeInUp fadeInUp">
                                                    <span>125</span>
                                                    <small>/ month</small>
